---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Vox Dolo</title>
 <link href="http://voxdolo.me/atom.xml" rel="self"/>
 <link href="http://voxdolo.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://voxdolo.me</id>
 <author>
   <name>Stephen Caudill</name>
   <email>voxdolo@me.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://voxdolo.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://voxdolo.me{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>

